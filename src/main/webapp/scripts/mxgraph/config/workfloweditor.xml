<mxEditor>
	<include name="config/wfeditor-commons.xml"/>
	<add as="onInit"><![CDATA[
		function ()
		{
			onInit(this);
		}
	]]></add>
	<ui>
		<add as="graph" element="graph"
			style="left:0px;right:0px;top:0px;bottom:0px"/>
		<!--  
		<add as="status" element="status"
			style="height:15px;bottom:20px;left:20px;right:100px"/>
		-->
		<add as="toolbar" x="16" y="20" width="50" style="padding:5px;padding-top:8px;padding-right:0px;z-index:200;"/>
	</ui>
	<mxGraph as="graph">
		<include name="config/wfgraph-commons.xml"/>
	</mxGraph>
	<mxDefaultToolbar as="toolbar">
		<include name="config/wftoolbar-commons.xml"/>
	</mxDefaultToolbar>
</mxEditor>
